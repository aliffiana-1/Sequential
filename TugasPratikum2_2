import java.util.Scanner;

public class TugasPratikum02_2 {
     public static void main(String[] args) {
        String cari;
        boolean found = false;
        String[] data = new String[]{"alif", "fiana", "rosa"};
        Scanner input = new Scanner(System.in);
        System.out.println("Masukkan data yang ingin ko cari!");
        cari = input.nextLine();
        for (int i = 0; i < data.length; i++) {
            if (cari == data[i]) {
                found = true;
                System.out.println("Data ditemukan pada indeks ke-" + 1);

            }

        }
        if (found != true) {
            System.out.println("Datanya ditemukannnn");

        }

    }

    
}
